[user]
    name = sindrehenriksen
    email = sindrehenriksen93@gmail.com

[alias]
    c = commit
    ca = commit --amend
    cf = commit --fixup
    s = status
    b = branch
    co = checkout
    l = log
    lo = log --oneline
    ps = push
    pnv = push --no-verify
    pl = pull
    d = diff
    a = add
    f = fetch
    ui = update-index
    uia = update-index --again
    sh = stash
    r = reset
    rb = rebase
    loga = log --oneline --decorate --graph --all --color
    lom = log --oneline master..
    fd = diff-tree --no-commit-id --name-only -r

[rebase]
    autosquash = true
    autostash = true

[pull]
    rebase = true
