Host gpu
    User sindre
    HostName 10.20.33.237
    IdentityFile ~/.ssh/id_rsa_gpu
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    XAuthLocation /usr/bin/xauth

Host gcp-tunnel
    User sindre_ragnarockgeo_com
    HostName internal.ragnardata.com
    IdentityFile ~/.ssh/id_rsa_gpu
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    XAuthLocation /usr/bin/xauth

Host gpu-public
    User sindre
    HostName localhost
    IdentityFile ~/.ssh/id_rsa_gpu
    Port 29855
    ProxyJump gcp-tunnel
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    XAuthLocation /usr/bin/xauth

Host github.com
    User git
    IdentityFile ~/.ssh/id_rsa_git
    AddKeysToAgent yes
